{
  "name": "react-form-layout",
  "version": "0.0.2",
  "description": "React's high order component for layouting form+fields",
  "main": "index.js",
  "scripts": {
    "test": "NODE_PATH=src:node_modules jest",
    "compile": "babel -d lib/ src/",
    "prepublish": "npm run compile"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alvinsj/react-form-layout.git"
  },
  "keywords": [
    "reactjs",
    "form",
    "layout",
    "builder",
    "react-component"
  ],
  "author": "email.to.alvin@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alvinsj/react-form-layout/issues"
  },
  "homepage": "https://github.com/alvinsj/react-form-layout",
  "dependencies": {
    "classnames": "^2.1.3",
    "object.assign": "^4.0.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.2"
  },
  "devDependencies": {
    "babel": "^6.0.15",
    "babel-cli": "^6.0.15",
    "babel-jest": "alvinsj/babel-jest#babel-6",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "jest-cli": "alvinsj/jest#quick-fix",
    "react-addons-test-utils": "^0.14.1"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "<rootDir>/__tests__"
    ],
    "testPathIgnorePatterns": [
      "pending"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "react-addons-test-utils"
    ],
    "modulePathIgnorePatterns": []
  }
}
