        "babel": "^6.5.x",
        "babel-cli": "^6.16.x",
        "babel-jest": "^15.x.x",
        "babel-preset-es2015": "^6.16.x",
        "babel-preset-react": "^6.16.x",
        "eslint": "^3.7.x",
        "eslint-plugin-react": "^6.x.x",
